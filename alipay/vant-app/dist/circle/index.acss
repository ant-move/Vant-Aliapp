.circle-index {
    display: block;
    height: initial;
}
@import "../common/index.acss";

.circle-index .van-circle {
    position: relative;
    display: inline-block;
    text-align: center;
}

.circle-index .van-circle__text {
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    color: #323233;
    color: var(--circle-text-color, #323233);
}
