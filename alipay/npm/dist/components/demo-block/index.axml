<view class='demo-block-index {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class="{{customClass}} demo-block van-clearfix {{ padding ? 'demo-block--padding' : '' }}">
    <view a:if='{{ title }}' class='demo-block__title'>
      {{ title }}
    </view>
    <view class='demo-block__card' a:if='{{ card }}'>
      <slot>
      </slot>
    </view>
    <slot a:else >
    </slot>
  </view>
</view>