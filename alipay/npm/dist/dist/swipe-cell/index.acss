.swipe-cell-index {
    display: block;
    height: initial;
}
@import "../common/index.acss";

.swipe-cell-index .van-swipe-cell {
    position: relative;
    overflow: hidden;
}

.swipe-cell-index .van-swipe-cell__left,
.swipe-cell-index .van-swipe-cell__right {
    position: absolute;
    top: 0;
    height: 100%;
}

.swipe-cell-index .van-swipe-cell__left {
    left: 0;
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
}

.swipe-cell-index .van-swipe-cell__right {
    right: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
}
