.panel-index {
    display: block;
    height: initial;
}
@import "../common/index.acss";

.panel-index .van-panel {
    background: #fff;
    background: var(--panel-background-color, #fff);
}

.panel-index .van-panel__header-value {
    color: #ee0a24;
    color: var(--panel-header-value-color, #ee0a24);
}

.panel-index .van-panel__footer {
    padding: 8px 16px;
    padding: var(--panel-footer-padding, 8px 16px);
}
