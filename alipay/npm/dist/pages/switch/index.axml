    </van-switch>
  </demo-block>
  <demo-block title='禁用状态' padding=" " ref='saveChildRef3'>
    <van-switch checked='{{ checked }}' disabled=" " onChange='onChange' ref='saveChildRef4'>
    </van-switch>
  </demo-block>
  <demo-block title='加载状态' padding=" " ref='saveChildRef5'>
    <van-switch checked='{{ checked }}' loading=" " onChange='onChange' ref='saveChildRef6'>
